		pinctrl-names = "default";
		pinctrl-0 = <&front_panel_led1_pins
			&front_panel_led2_pins
			&front_panel_led3_pins
			&front_panel_led4_pins
			&front_panel_led5_pins
			&front_panel_led6a_pins
