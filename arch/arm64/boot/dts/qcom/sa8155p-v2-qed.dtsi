			&front_panel_led7a_pins
			&front_panel_led7b_pins
			&front_panel_led8a_pins
			&front_panel_led8b_pins
			&front_panel_inner_led1_pins
			&front_panel_inner_led2_pins
			&front_panel_inner_led3_pins
			&front_panel_inner_led4_pins
			&front_panel_inner_led5_pins>;

		front_panel_led1_pins: gpio1-led {
			pins = "gpio0";
