				      "axi_s_sticky",
				      "axi_s",
				      "axi_m_sticky",
				      "axi_m",
				      "aux",
				      "ahb";

			msi-parent = <&v2m2>;
			nvmem-cells = <&pcie3_disable>;
			status = "disabled";
		};

