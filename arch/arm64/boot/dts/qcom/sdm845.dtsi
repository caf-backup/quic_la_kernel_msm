				< 1689600 1305600 >,
				< 1766400 1401600 >,
				< 2745600 1593600 >;
		};

		cpubw-cpufreq {
		       target-dev = <&cpubw>;
		       cpu-to-dev-map =
				<  300000 1171875 >,
				<  422400 2660156 >,
				<  652800 3000000 >,
				<  883200 6074218 >,
