#include "qcom-ipq5018.dtsi"

/ {
	#address-cells = <0x2>;
	#size-cells = <0x2>;
	model = "Qualcomm Technologies, Inc. IPQ5018-EMULATION-C1";
	compatible = "qcom,ipq5018-emulation-c1", "qcom,ipq5018";
	interrupt-parent = <&intc>;

	aliases {
		sdhc1 = &sdhc_1; /* SDC1 eMMC slot */
		serial0 = &blsp1_uart1;
	};

	chosen {
		linux,initrd-end = <0x46000000>;
		linux,initrd-start = <0x44000000>;
		bootargs = "root=/dev/ram0 rw init=/init nosmp";
		stdout-path = "serial0";
	};

	memory {
		device_type = "memory";
		reg = <0x0 0x40000000 0x0 0x20000000>;
	};

	soc {
		serial@78af000 {
			status = "ok";
		};

		timer {
			clock-frequency = <240000>;
		};

		spi_0: spi@78b5000 { /* BLSP1 QUP0 */
			status = "ok";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				reg = <0>;
				compatible = "n25q128a11";
				linux,modalias = "m25p80", "n25q128a11";
				spi-max-frequency = <50000000>;
				use-default-sizes;
			};
		};

		ess-switch@0x39c00000 {
			compatible = "qcom,ess-switch-ipq50xx";
			reg = <0x39c00000 0x200000>;
			switch_access_mode = "local bus";
			qcom,port_phyinfo {
				port@0 {
					port_id = <1>;
					phy_address = <7>;
				};
				port@1 {
					port_id = <2>;
					phy_address = <1>;
				};
			};
		};

		qca,scm_restart_reason {
			status = "disabled";
		};
	};
};

&sdhc_1 {
	qcom,clk-rates = <400000 25000000 50000000 100000000 \
			 192000000 384000000>;
	qcom,bus-speed-mode = "DDR_1p8v";
	qcom,nonremovable;
	qcom,emulation = <1>;
	status = "ok";
};

&qcom_q6v5_wcss {
	qcom,nosecure;
};
